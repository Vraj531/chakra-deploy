{
	"name": "chakra-deploy",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "npm run build && wrangler pages dev --port 5173",
		"serve:db": "drizzle-kit studio --config=dev-drizzle.config.ts",
		"dev:push": "drizzle-kit push --config=dev-drizzle.config.ts",
		"prod:db:push": "drizzle-kit push --config=drizzle.config.ts",
		"dev:generate": "drizzle-kit generate --config=dev-drizzle.config.ts",
		"dev:migrate": "drizzle-kit migrate --config=dev-drizzle.config.ts",
		"prod:db": "drizzle-kit studio --config=drizzle.config.ts",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "npm run build && wrangler pages deploy",
		"cf-typegen": "wrangler types && mv worker-configuration.d.ts src/"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.582.0",
		"@aws-sdk/s3-request-presigner": "^3.582.0",
		"@aws-sdk/util-create-request": "^3.582.0",
		"@aws-sdk/util-format-url": "^3.577.0",
		"@cloudflare/workers-types": "^4.20240512.0",
		"@poppanator/sveltekit-svg": "^4.2.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-cloudflare": "^4.4.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^8.56.0",
		"@types/gtag.js": "^0.0.19",
		"@types/sanitize-html": "^2.11.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.11.1",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.21.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"saos": "^1.3.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"wrangler": "^3.57.0"
	},
	"type": "module",
	"dependencies": {
		"@libsql/client": "^0.6.0",
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"@types/nprogress": "^0.2.3",
		"@vercel/analytics": "^1.3.1",
		"arctic": "^1.8.1",
		"axios": "^1.7.2",
		"drizzle-orm": "^0.30.10",
		"embla-carousel-svelte": "^8.1.3",
		"lucia": "^3.2.0",
		"nprogress": "^0.2.0",
		"sanitize-html": "^2.13.0"
	}
}
